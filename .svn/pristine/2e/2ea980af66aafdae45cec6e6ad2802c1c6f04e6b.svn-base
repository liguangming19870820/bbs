package com.xxt.entity;

import com.xxt.common.base.BaseEntity;

public class Img extends BaseEntity {

    private static final long serialVersionUID = 1L;

    /** ID */
    private Integer id;
    /** 商品ID */
    private Integer productId;
    /** 图片URL */
    private String url;
    /** 是否默认:0否 1:是 */
    private Short isDef;

    /*field end*/

    public Integer getId() {
        return id;
    }
    public void setId(Integer id) {
        this.id = id;
    }
    public Integer getProductId() {
        return productId;
    }
    public void setProductId(Integer productId) {
        this.productId = productId;
    }
    public String getUrl() {
        return url;
    }
    public void setUrl(String url) {
        this.url = url;
    }
    public Short getIsDef() {
        return isDef;
    }
    public void setIsDef(Short isDef) {
        this.isDef = isDef;
    }

    /*method end*/

	@Override
	public String toString() {
		return "Img [id=" + id + ", productId=" + productId + ", url=" + url + ", isDef=" + isDef+"]";
	}
}
